DROP TABLE CoursesAndDomains;
DROP TABLE Domains;
DROP TABLE Courses;

CREATE TABLE Domains(
ID int NOT NULL AUTO_INCREMENT,
DomainName varchar(100) NOT NULL UNIQUE,
PRIMARY KEY (ID)
);

CREATE TABLE Courses(
ID int NOT NULL AUTO_INCREMENT,
CourseName varchar(100) NOT NULL UNIQUE,
PRIMARY KEY (ID)
);

CREATE TABLE CoursesAndDomains(
ID int NOT NULL AUTO_INCREMENT,
IDDomain int NOT NULL,
IDCourse int NOT NULL,
FOREIGN KEY (IDDomain) REFERENCES Domains(ID),
FOREIGN KEY (IDCourse) REFERENCES Courses(ID),
PRIMARY KEY (ID)
);

